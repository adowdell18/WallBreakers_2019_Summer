class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        
        sum_hash = {}
        for ind, c_num in enumerate (nums):
            if target - c_num in sum_hash:
                return ([sum_hash[target-c_num], ind])
                break
            sum_hash[c_num] = ind
        return ([])
