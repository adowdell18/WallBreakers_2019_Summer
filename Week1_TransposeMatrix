t = []

class Solution:
    def transpose(self, A):
        """
        :type A: List[List[int]]
        :rtype: List[List[int]]
        """
        #t = [[A[j][i] for j in range (len(A)) for i in range (len[A[0]])]]
        t = [[A[j][i] for j in range(len(A))] for i in range(len(A[0]))]
        
        return t
